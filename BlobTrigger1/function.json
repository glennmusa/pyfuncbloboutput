{
  "scriptFile": "__init__.py",
  "bindings": [
    {
      "name": "myblob",
      "type": "blobTrigger",
      "direction": "in",
      "path": "sample-images/{name}",
      "connection": "YOUR_IN_STORAGE_ACCOUNT_CONNECTION_STRING"
    },
    {
      "name": "outputblob",
      "type": "blob",
      "path": "sample-images-copy/{name}",
      "connection": "YOUR_OUT_STORAGE_ACCOUNT_CONNECTION_STRING",
      "direction": "out"
    }
  ]
}